{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"webview:default",
		"app:default",
		"resources:default",
		"menu:default",
		"tray:default",

    "sql:allow-load",
    "sql:allow-close",
    "sql:allow-execute",
    "sql:allow-select"
	]
}
